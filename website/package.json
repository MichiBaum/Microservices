{
  "name": "website",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host=127.0.0.1",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build-production": "ng build --configuration production",
    "lint": "ng lint",
    "bundle-report": "ng build --configuration bundle-report --source-map && source-map-explorer ../website-service/src/main/resources/static/*.js",
